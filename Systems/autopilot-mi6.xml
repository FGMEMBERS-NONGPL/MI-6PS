    </reference>
    <output>
      <prop>/controls/flight/aileron-trim-filter</prop>
    </output>
    <config>
      <Kp>25.0</Kp><!-- 0.015 -->
      <beta>1.0</beta>
      <alpha>0.1</alpha>
      <gamma>0.0</gamma>
      <Ti>10.0</Ti><!-- 10.0 -->
