	</equals>
      </condition>
    </enable>
    <input>
      <property>/autopilot/internal/true-heading-error-deg</property>
    </input>
    <reference>
      <value>0.0</value>
    </reference>
    <output>
      <property>autopilot/internal/target-roll-filter</property>
    </output>
    <config>
      <Kp>-1.0</Kp>
      <Ki>0.0</Ki>
      <min>
	<value>-25</value>
      </min>
      <max>
	<value>25</value>
      </max>
    </config>
  </pi-simple-controller>
  
  <pi-simple-controller>
    <name>NAV 1 Hold</name>
    <debug>false</debug>
