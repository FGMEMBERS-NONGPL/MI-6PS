	<!--Main Engines Cover and Switch-->
	
	<animation>
		<type>pick</type>
		<object-name>Plane.016</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/switches/engines-main-cover</property>
				<value>0</value>
				<value>1</value>
			</binding>
		</action>
	</animation>
	
	<animation>
	  <type>rotate</type>
	  <object-name>Plane.016</object-name>
	  <property>controls/switches/engines-main-cover</property>
	  <factor>-90</factor>
	  <center>
	    <x-m>-5.775</x-m>
	    <y-m>-0.148</y-m>
	    <z-m>3.600</z-m>
	  </center>
	  <axis>
	    <x>0</x>
	    <y>1</y>
	    <z>0</z>
	  </axis>
	</animation>
	
	<animation>
	  <type>pick</type>
	  <object-name>Switch.188</object-name>
	  <action>
	    <button>0</button>
	    <repeatable>false</repeatable>
	    <binding>
	      <command>nasal</command>
	      <script>mi6.engines(1)</script>
	    </binding>
	  </action>
	</animation>
	
	<animation>
	  <type>pick</type>
	  <object-name>Switch.188</object-name>
	  <action>
	    <button>1</button>
	    <repeatable>false</repeatable>
	    <binding>
	      <command>nasal</command>
	      <script>mi6.engines(0)</script>
	    </binding>
	  </action>
	</animation>
	
	<animation>
	  <type>rotate</type>
	  <object-name>Switch.188</object-name>
	  <property>sim/model/mi6/engine</property>
	  <factor>-90</factor>
	  <center>
	    <x-m>-5.801</x-m>
	    <y-m>0.139</y-m>
	    <z-m>3.605</z-m>
	  </center>
	  <axis>
	    <x>0</x>
	    <y>1</y>
	    <z>0</z>
	  </axis>
	</animation>
	
	<!--Rotor Brake Switch-->
	<animation>
	  <type>pick</type>
	  <object-name>Switch.187</object-name>
	  <action>
	    <button>0</button>
	    <repeatable>false</repeatable>
	    <binding>
	      <command>nasal</command>
	      <script>interpolate("/controls/rotor/brake", 1.0, 2)</script>
	    </binding>
	  </action>
	</animation>
	
	<animation>
	  <type>pick</type>
	  <object-name>Switch.187</object-name>
	  <action>
	    <button>1</button>
	    <repeatable>false</repeatable>
	    <binding>
	      <command>nasal</command>
	      <script>interpolate("/controls/rotor/brake", 0.0, 2)</script>
	    </binding>
	  </action>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>Switch.187</object-name>
		<property>/controls/rotor/brake</property>
		<factor>-90</factor>
		<center>
			<x-m>-5.801</x-m>
			<y-m>0.179</y-m>
			<z-m>3.605</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<!--Main Attitude Indicator Switch-->
	<animation>
		<type>pick</type>
		<object-name>main_ai</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>instrumentation/attitude-indicator[0]/power</property>
				<value>0</value>
				<value>1</value>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>main_ai</object-name>
		<property>instrumentation/attitude-indicator[0]/power</property>
		<factor>-90</factor>
		<center>
			<x-m>-5.590</x-m>
			<y-m>0.032</y-m>
			<z-m>3.658</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
